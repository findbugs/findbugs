<?xml version="1.0" encoding="UTF-8"?>

<MessageCollection xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="messagecollection.xsd">

  <Plugin>
    <ShortDescription>Power user command line tools</ShortDescription>
    <Details>
<![CDATA[
<p>
Poweruser command line tools
</p>
]]>
    </Details>
   </Plugin>
    

<FindBugsMain cmd="copySource" class="edu.umd.cs.findbugs.workflow.CopyBuggySource">
    <Description>Copy all the source files referenced in analysis results to a src directory</Description>
  </FindBugsMain>
  <FindBugsMain cmd="rejar" class="edu.umd.cs.findbugs.workflow.RejarClassesForAnalysis">
    <Description>Combine multiple jar files into jar files for analysis</Description>
  </FindBugsMain>
  <FindBugsMain cmd="dd" class="edu.umd.cs.findbugs.workflow.DefectDensity">
    <Description>List the defect density by package and class</Description>
  </FindBugsMain>
  <FindBugsMain cmd="msv" class="edu.umd.cs.findbugs.workflow.MergeSummarizeAndView">
    <Description>Merge several analysis results, synchronize with cloud, list new high rank issues and optionally open GUI</Description>
  </FindBugsMain>
  <FindBugsMain cmd="backdate" class="edu.umd.cs.findbugs.workflow.BackdateHistoryUsingSource">
    <Description>Use the last-modified date of source files to back date the introduction
            of bugs</Description>
  </FindBugsMain>
  <FindBugsMain cmd="csr" class="edu.umd.cs.findbugs.workflow.CloudSyncAndReport">
    <Description>Synchronize analysis results with cloud and report</Description>
  </FindBugsMain>
  <FindBugsMain cmd="appVersion" class="edu.umd.cs.findbugs.workflow.PrintAppVersion">
    <Description>Print the AppVersion information from a BugCollection</Description>
  </FindBugsMain>
  
    <FindBugsMain cmd="xpath" class="edu.umd.cs.findbugs.xml.XPathFind">
    <Description>Perform an xpath query on an XML file</Description>
  </FindBugsMain>

  <FindBugsMain cmd="stats" class="edu.umd.cs.findbugs.workflow.ListBugDatabaseInfo">
    <Description>List stats from several analysis files (num classes, code size, num issues found)</Description>
  </FindBugsMain>
  
  </MessageCollection>



